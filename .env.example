# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Application Settings
NODE_ENV=development
VITE_APP_TITLE=Patunay
VITE_APP_ENV=development

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
VITE_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_ENABLE_DEVTOOLS=true
VITE_LOG_LEVEL=debug

# Electron Configuration
ELECTRON_MAIN_LOG_LEVEL=debug
ELECTRON_RENDERER_LOG_LEVEL=debug

# Optional: Analytics
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# Network Connectivity Configuration
# Endpoint to test network connectivity (default: https://www.google.com/favicon.ico)
# To use your Supabase endpoint: VITE_NETWORK_TEST_ENDPOINT=${VITE_SUPABASE_URL}/rest/v1/
VITE_NETWORK_TEST_ENDPOINT=https://www.google.com/favicon.ico
# CORS mode for network test: 'cors', 'no-cors', or 'same-origin' (default: no-cors)
VITE_NETWORK_TEST_CORS_MODE=no-cors
# Whether to use authentication for network test (required for Supabase endpoints)
VITE_NETWORK_TEST_USE_AUTH=false
# Interval for network connectivity checks in milliseconds (default: 30000 = 30 seconds)
# Set to 0 to disable periodic checks
VITE_NETWORK_TEST_INTERVAL=30000