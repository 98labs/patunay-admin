{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "../../dist-electron",
    "rootDir": "..",
    "strict": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "electron"],
    "resolveJsonModule": true,
    "noImplicitAny": false
  },
  "include": ["preload.ts"]
}